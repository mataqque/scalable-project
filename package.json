		"@types/react-lottie": "^1.2.6",
		"@types/styled-components": "^5.1.26",
		"array-move": "^4.0.0",
		"axios": "^1.2.0",
		"formik": "^2.2.9",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-dropzone": "^14.2.3",
