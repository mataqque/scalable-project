		"array-move": "^4.0.0",
		"axios": "^1.2.0",
		"formik": "^2.2.9",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-dropzone": "^14.2.3",
		"react-helmet-async": "^1.3.0",
		"react-lottie": "^1.2.3",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.4.4",
		"react-sortable-hoc": "^2.0.0",
		"react-toastify": "^9.1.1",
