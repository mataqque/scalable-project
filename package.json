{
	"name": "infinix-frontend",
	"private": true,
	"version": "latest",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"format": "prettier --write .",
		"lint": "eslint ./src",
		"test": "vitest",
		"coverage": "vitest run --coverage",
